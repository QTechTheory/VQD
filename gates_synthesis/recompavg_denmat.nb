 RowBox[{
  RowBox[{
   RowBox[{"twoRGate", "::", "usage"}], "=", 
   "\"\<twoRGate[conf, graphavg]. graphavg contains weight that is \
normalized. Pick a or multiple random two-qubit gates. When the connectivity \
isn't possible, return a chain of gates. This assumes every 2-qubit gates \
have the same connectivity\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"twoRGate", "[", 
    RowBox[{"conf_", ",", "graphavg_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"pool", ",", "gate", ",", "qubits", ",", "conn", ",", 
       RowBox[{"path", "=", 
        RowBox[{"{", "}"}]}], ",", "pathpair", ",", 
       RowBox[{"gates", "=", 
        RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"pool", "=", 
       RowBox[{
        RowBox[{"conf", "[", "\"\<gateset\>\"", "]"}], "[", "2", "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"pool", "=", 
       RowBox[{"RandomSample", "[", "pool", "]"}]}], ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{"randomly", " ", "permute"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"conn", "=", 
       RowBox[{"conf", "[", "\"\<connectivity\>\"", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "@", "path"}], "<", "1"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"*", " ", "draw", " ", "a", " ", "pair", " ", "of", " ", 
          RowBox[{"qubits", ".", " ", "potentially"}], " ", "construct", " ", 
          "preferable", " ", "gate", " ", "operation", " ", "and", " ", 
          "\[IndentingNewLine]", "subspace", " ", "by", " ", "using", " ", 
          "subgraph"}], " ", "**)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"qubits", "=", 
          RowBox[{"Level", "[", 
           RowBox[{
            RowBox[{"RandomChoice", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"PropertyValue", "[", 
                RowBox[{"graphavg", ",", "EdgeLabels"}], "]"}], "[", 
               RowBox[{"[", 
                RowBox[{"All", ",", "2"}], "]"}], "]"}], "->", 
              RowBox[{
               RowBox[{"PropertyValue", "[", 
                RowBox[{"graphavg", ",", "EdgeLabels"}], "]"}], "[", 
               RowBox[{"[", 
                RowBox[{"All", ",", "1"}], "]"}], "]"}]}], "]"}], ",", 
            RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"qubits", "=", 
          RowBox[{"RandomSample", "[", "qubits", "]"}]}], ";", " ", 
         RowBox[{"(*", "shuffle", "*)"}], "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "find", " ", "a", " ", "path", " ", "given", " ", "connectivity"}], 
          "*)"}], "\[IndentingNewLine]", 
         RowBox[{"path", "=", 
          RowBox[{"FindShortestPath", "[", 
           RowBox[{"conn", ",", 
            RowBox[{"Sequence", "@@", "qubits"}]}], "]"}]}], ";"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"gate", "=", 
       RowBox[{"RandomChoice", "[", "pool", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"pathpair", "=", 
       RowBox[{"ReplaceList", "[", 
        RowBox[{"path", ",", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"p___", ",", "a_", ",", "b_", ",", "q___"}], "}"}], ":>", 
          RowBox[{"{", 
           RowBox[{"a", ",", "b"}], "}"}]}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"AppendTo", "[", 
       RowBox[{"gates", ",", 
        RowBox[{
         RowBox[{"RandomChoice", "[", "pool", "]"}], "@@", 
         RowBox[{"pathpair", "[", 
          RowBox[{"[", "1", "]"}], "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "@", 
          RowBox[{"conf", "[", "\"\<swap\>\"", "]"}]}], "===", "Function"}], 
        ",", "\[IndentingNewLine]", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"gates", ",", 
            RowBox[{
             RowBox[{"conf", "[", "\"\<swap\>\"", "]"}], "@@", "pair"}]}], 
           "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"pair", ",", 
            RowBox[{"pathpair", "[", 
             RowBox[{"[", 
              RowBox[{"2", ";;"}], "]"}], "]"}]}], "}"}]}], "]"}], 
        "\[IndentingNewLine]", ",", "\[IndentingNewLine]", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"gates", ",", 
            RowBox[{
             RowBox[{"RandomChoice", "[", "pool", "]"}], "@@", "pair"}]}], 
           "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"pair", ",", 
            RowBox[{"pathpair", "[", 
             RowBox[{"[", 
              RowBox[{"2", ";;"}], "]"}], "]"}]}], "}"}]}], "]"}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "gates"}]}], 
    "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
