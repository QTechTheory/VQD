        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"Ecur", ",", "\[Theta]vars"}], "}"}], "=", 
          RowBox[{"{", 
           RowBox[{"Einit", ",", "\[Theta]varsinit"}], "}"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Elist", "=", 
          RowBox[{"{", "Einit", "}"}]}], ";"}]}], "\[IndentingNewLine]", 
       "]"}], ";", "\[IndentingNewLine]", "Elist"}], "\[IndentingNewLine]", 
