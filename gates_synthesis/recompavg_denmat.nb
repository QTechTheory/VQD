 RowBox[{
  RowBox[{"singleRGate", "[", 
   RowBox[{"conf_", ",", "probzeros_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"pool", ",", " ", "gate", ",", "qubit", ",", "npool"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
     "potentially", " ", "construct", " ", "preferable", " ", "gate", " ", 
      "operation"}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"pool", "=", 
      RowBox[{
       RowBox[{"conf", "[", "\"\<gateset\>\"", "]"}], "[", "1", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pool", "=", 
      RowBox[{"RandomSample", "[", "pool", "]"}]}], ";", " ", 
     RowBox[{"(*", " ", 
      RowBox[{"randomly", " ", "permute"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"gate", "=", 
      RowBox[{"RandomChoice", "[", "pool", "]"}]}], ";", 
     "\[IndentingNewLine]", 
