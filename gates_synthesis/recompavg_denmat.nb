Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ExtendAnsatz", "::", "usage"}], "=", 
   "\"\<ExtendAnsatz[idx, conf, ansatz, naddgates].\nExtend ansatz by adding \
naddgates gates. There is no redundancy check here. \nArguments aren't \
changed, except the start index. Return the new ansatz candidate.\nAnsatz is \
done 50% globally spread, 25% appended at the end, and 25% appended in the \
begining.\nReturn: extended ansatz\n\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"SetAttributes", "[", 
  RowBox[{"ExtendAnsatz", ",", "HoldFirst"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ExtendAnsatz", "[", 
   RowBox[{
   "idx_", ",", "conf_", ",", "ansatz_", ",", "naddgates_", ",", "args___"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"linit", "=", 
       RowBox[{"Length", "@", "ansatz"}]}], ",", 
      RowBox[{"ansatztmp", "=", "ansatz"}], ",", "rgates", ",", "method", ",",
       "ngates", ",", "ngatestotal", ",", "probzeros", ",", "qmiavg", ",", 
      "sloc"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"method", ":", " ", 
       RowBox[{"1", "-", "global"}]}], ",", " ", 
      RowBox[{"2", "-", "appended"}], ",", " ", 
      RowBox[{"3", "-", "prepended"}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"method", "=", 
      RowBox[{"RandomChoice", "[", 
       RowBox[{"{", 
        RowBox[{"1", ",", "2", ",", "3"}], "}"}], " ", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"ngatestotal", "=", 
      RowBox[{"naddgates", "+", 
       RowBox[{"Length", "@", "ansatz"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"conf", "[", "\"\<usecorr\>\"", "]"}], ",", 
       "\[IndentingNewLine]", "probzeros", ",", 
       RowBox[{
        RowBox[{"qmiavg", "=", 
         RowBox[{"{", "args", "}"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "@", "ansatztmp"}], "<", "ngatestotal"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"introduce", " ", "random", " ", "gates"}], "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"rgates", "=", 
         RowBox[{"Flatten", "@", 
          RowBox[{"{", 
           RowBox[{"RandomGate", "[", 
            RowBox[{"conf", ",", "args"}], "]"}], "}"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"location", " ", "of", " ", "new", " ", "gates"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"Which", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"method", "===", "1"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"sloc", "=", 
            RowBox[{"RandomChoice", "[", 
             RowBox[{"Range", "[", 
              RowBox[{
               RowBox[{"Length", "@", "ansatztmp"}], "+", "1"}], "]"}], 
             "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"ansatztmp", "=", 
              RowBox[{"Insert", "[", 
               RowBox[{"ansatztmp", ",", "g", ",", "sloc"}], "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"g", ",", "rgates"}], "}"}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"method", "===", "2"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"ansatztmp", "=", 
            RowBox[{"Join", "[", 
             RowBox[{"rgates", ",", "ansatztmp"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"method", "===", "3"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"ansatztmp", "=", 
           RowBox[{"Join", "[", 
            RowBox[{"ansatztmp", ",", "rgates"}], "]"}]}]}], 
         "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"ReplaceAll", "[", 
      RowBox[{"ansatztmp", ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\[Theta]_Subscript", "\[RuleDelayed]", "\[Theta]"}], ",", 
