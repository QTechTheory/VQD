Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"prepareTrainInit", "::", "usage"}], "=", 
   "\"\<PrepareTrainInit[unitary, ntrainstates], returns register identity of \
a set of initial states { {\!\(\*SubscriptBox[\(\[Rho]\), \
SubscriptBox[\(PU\), \(j\)]]\)}, {\!\(\*SuperscriptBox[\(P\), \
\(\[Dagger]\)]\)} }, where {P} random Paulis.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"prepareTrainInit", "[", 
    RowBox[{"unitary_", ",", "nqubits_", ",", "ntrainstates_"}], "]"}], ":=", 
   
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "u", ",", "\[Rho]s", ",", "paulib", ",", "rpaulis", ",", "p", ",", "e", 
       ",", 
       RowBox[{"st", "=", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"invop", "=", 
        RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"paulib", "=", 
       RowBox[{"<|", " ", 
        RowBox[{
         RowBox[{"\"\<x\>\"", "->", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", "H", "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"H", ",", "Z"}], "}"}]}], "}"}]}], ",", 
         RowBox[{"\"\<y\>\"", "->", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"H", ",", "S"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"H", ",", "Z", ",", "S"}], "}"}]}], "}"}]}], ",", 
         RowBox[{"\"\<z\>\"", "->", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", "}"}], ",", 
            RowBox[{"{", "X", "}"}]}], "}"}]}]}], "|>"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"rpaulis", "=", 
       RowBox[{"<|", 
        RowBox[{
         RowBox[{"\"\<x\>\"", "->", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", "H", "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"Z", ",", "H"}], "}"}]}], "}"}]}], ",", 
         RowBox[{"\"\<y\>\"", "->", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"Z", ",", "S", ",", "H"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"S", ",", "H"}], "}"}]}], "}"}]}], ",", 
         RowBox[{"\"\<z\>\"", "->", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", "}"}], ",", 
            RowBox[{"{", "X", "}"}]}], "}"}]}]}], "|>"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"u", "=", 
       RowBox[{
        SubscriptBox["U", 
         RowBox[{"Range", "[", 
          RowBox[{"0", ",", 
           RowBox[{
            RowBox[{"-", "1"}], "+", 
            RowBox[{"Log2", "@", 
             RowBox[{"Length", "@", "unitary"}]}]}]}], "]"}]], "[", 
        RowBox[{"ConjugateTranspose", "@", "unitary"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Rho]s", "=", 
       RowBox[{"CreateDensityQuregs", "[", 
        RowBox[{"nqubits", ",", "ntrainstates"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"<|", "\[IndentingNewLine]", 
       RowBox[{"Table", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"st", "=", 
           RowBox[{"{", "}"}]}], ";", 
          RowBox[{"invop", "=", 
           RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
          RowBox[{"Table", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"p", "=", 
              RowBox[{"RandomChoice", "@", 
               RowBox[{"Keys", "@", "paulib"}]}]}], ";", " ", 
             RowBox[{"(*", 
              RowBox[{"random", " ", "basis"}], "*)"}], "\[IndentingNewLine]", 
             RowBox[{"e", "=", 
              RowBox[{"RandomChoice", "@", 
               RowBox[{"{", 
                RowBox[{"1", ",", "2"}], "}"}]}]}], ";", " ", 
             RowBox[{"(*", 
              RowBox[{"random", " ", "eigenstate"}], "*)"}], 
             "\[IndentingNewLine]", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"st", ",", 
               RowBox[{
                RowBox[{
                 RowBox[{"Subscript", "[", 
                  RowBox[{"#", ",", "q"}], "]"}], "&"}], "/@", 
                RowBox[{
                 RowBox[{"paulib", "[", "p", "]"}], "[", 
                 RowBox[{"[", "e", "]"}], "]"}]}]}], "]"}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"invop", ",", 
               RowBox[{
                RowBox[{
                 RowBox[{"Subscript", "[", 
                  RowBox[{"#", ",", "q"}], "]"}], "&"}], "/@", " ", 
                RowBox[{
                 RowBox[{"rpaulis", "[", "p", "]"}], "[", 
                 RowBox[{"[", "e", "]"}], "]"}]}]}], "]"}], ";"}], 
            "\[IndentingNewLine]", ",", 
            RowBox[{"{", 
             RowBox[{"q", ",", "0", ",", 
              RowBox[{"nqubits", "-", "1"}]}], "}"}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"ApplyCircuit", "[", 
           RowBox[{
            RowBox[{"\[Rho]s", "[", 
             RowBox[{"[", "n", "]"}], "]"}], ",", 
            RowBox[{"Flatten", "@", 
             RowBox[{"{", 
              RowBox[{"st", ",", "u"}], "}"}]}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"n", "->", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"\[Rho]s", "[", 
              RowBox[{"[", "n", "]"}], "]"}], ",", "st", ",", 
             RowBox[{"Flatten", "@", "invop"}]}], "}"}]}]}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "ntrainstates"}], "}"}]}], "]"}], 
       "\[IndentingNewLine]", "|>"}]}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
