Cell[BoxData[{
 RowBox[{"SetAttributes", "[", 
  RowBox[{"CompileCirc", ",", "HoldAll"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"CompileCirc", "[", 
   RowBox[{"conf_", ",", "unitary_", ",", "qubitsnum_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "iansatz", ",", "nansatz", ",", "simpncirc", ",", "uapprox", ",", "uvc", 
      ",", "uec", ",", "fdist"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"time", ",", 
        RowBox[{"{", 
         RowBox[{
         "Elist", ",", "ansatz", ",", "\[Theta]vars", ",", "finmsg", ",", 
          "ncycle", ",", "fev", ",", "aborted", ",", "cycleres", ",", 
          "probzeros", ",", "qmiavg", ",", "elimmerge", ",", "elimbfsmall", 
          ",", "elimmetov", ",", "elimbf"}], "}"}]}], "}"}], "=", 
      RowBox[{"Timing", "[", 
       RowBox[{"CircuitSynthesis", "[", 
        RowBox[{"qubitsnum", ",", "conf"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"iansatz", "=", 
      RowBox[{
       RowBox[{"Reverse", "[", "ansatz", "]"}], "/.", 
       RowBox[{"(", 
        RowBox[{"-", "\[Theta]vars"}], ")"}]}]}], ";", "\n", 
     RowBox[{"nansatz", "=", 
      RowBox[{"noisyAnsatz", "[", 
       RowBox[{"iansatz", ",", "conf"}], "]"}]}], ";", "\n", 
     RowBox[{"simpncirc", "=", 
      RowBox[{"SimplifyCircuit", "[", 
       RowBox[{"DeleteCases", "[", 
        RowBox[{
         RowBox[{"DeleteCases", "[", 
          RowBox[{"nansatz", ",", 
           RowBox[{
            SubscriptBox["Depol", "__"], "[", "0.", "]"}]}], "]"}], ",", 
         RowBox[{
          SubscriptBox["Deph", "__"], "[", "0.", "]"}]}], "]"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"uapprox", "=", 
      RowBox[{"CalcCircuitMatrix", "[", "simpncirc", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"fidelity", " ", "distance"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"uvc", "=", 
      RowBox[{"Flatten", "@", 
       RowBox[{"ConjugateTranspose", "@", "unitary"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{"Length", "@", "uapprox"}], ",", 
       RowBox[{"Length", "@", "uvc"}]}], "]"}], ";", "\n", 
     RowBox[{"uec", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"#", ".", "uvc"}], "&"}], "/@", "uapprox"}]}], ";", "\n", 
     RowBox[{"fdist", "=", 
      RowBox[{
       RowBox[{"Total", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Chop", "[", 
           RowBox[{"#", "*", 
            RowBox[{"Conjugate", "[", "#", "]"}]}], "]"}], "&"}], "/@", 
         "uec"}], "]"}], "/", 
       RowBox[{"Length", "@", "uec"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<compile time:\>\"", ",", 
       RowBox[{"time", "/", "60"}], ",", 
       "\"\< minutes; total iterations \>\"", ",", "fev"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "@", 
      RowBox[{"ListPlot", "[", 
       RowBox[{"Elist", ",", 
        RowBox[{"AxesLabel", "->", 
         RowBox[{"{", 
          RowBox[{"\"\<Successful iteration\>\"", ",", "\"\<Cost\>\""}], 
          "}"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "@", 
      RowBox[{"DrawCircuit", "[", 
       RowBox[{"iansatz", ",", "qubitsnum"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<prob(0):\>\"", ",", "probzeros"}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
      "\"\<\!\(\*SubscriptBox[\(dist\), \(fid\)]\)=\>\"", ",", "fdist"}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
